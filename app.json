{
  "expo": {
    "scheme": "ROK",
    "name": "ROK",
    "slug": "ROK",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "backgroundColor": "#222222",


    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-video",
      ["expo-splash-screen",
      {
        "backgroundColor": "#000000",
        "image": "Boulder/assets/RockInactive.png",
        "imageWidth": 200
      }]
    ]
  }
}
